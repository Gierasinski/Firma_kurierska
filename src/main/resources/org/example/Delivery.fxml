      <TextField fx:id="tfpackagenumberD" layoutX="215.0" layoutY="60.0" prefHeight="27.0" prefWidth="220.0" styleClass="textField" stylesheets="@style.css" />
      <Label layoutX="452.0" layoutY="59.0" styleClass="text" stylesheets="@style.css" text="package number" textFill="WHITE">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button fx:id="btnpickingupthepackagef" layoutX="24.0" layoutY="52.0" mnemonicParsing="false" onAction="#pickupthepackage0" prefHeight="62.0" prefWidth="172.0" styleClass="button2" stylesheets="@style.css" text="Picking up the package">
         <font>
            <Font size="14.0" />
